import java.util.*;

class Solution {
    public void rearrangeQueue(Queue<Integer> q) {
        int n = q.size();
        Queue<Integer> first = new LinkedList<>();

        for (int i = 0; i < n / 2; i++) {
            first.add(q.poll());
        }

        while (!first.isEmpty()) {
            q.add(first.poll());
            q.add(q.poll());
        }
    }
}
