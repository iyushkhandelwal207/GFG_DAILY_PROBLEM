import java.util.*;

class Solution {
    public ArrayList<String> generateBinary(int n) {
        ArrayList<String> result = new ArrayList<>();
        
        // Loop from 1 to n and convert each to binary
        for (int i = 1; i <= n; i++) {
            result.add(Integer.toBinaryString(i));
        }
        
        return result;
    }
}
